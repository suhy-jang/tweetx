{
  "name": "tweetx",
  "version": "1.1.0",
  "description": "Twitter functional clone project",
  "main": "index.js",
  "directories": {
    "doc": "docs"
  },
  "scripts": {
    "start": "node dist",
    "build": "npm run build:client",
    "heroku-postbuild": "babel src --out-dir dist --copy-files && NPM_CONFIG_PRODUCTION=false && npm install --prefix client && npm run build --prefix client",
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "client": "npm start --prefix client",
    "server": "NODE_ENV=development env-cmd -f ./config/dev.env nodemon src --ext js, graphql --exec babel-node --ignore client",
    "seed": "NODE_ENV=development env-cmd -f ./config/dev.env babel-node src --ext js, graphql",
    "test": "env-cmd -f ./config/test.env jest --watch --runInBand --notify",
    "get-schema": "graphql get-schema -p prisma --dotenv config/dev.env --no-all",
    "migrate": "env-cmd -f ./config/dev.env prisma migrate dev --schema=./prisma/schema.postgresql.prisma",
    "build:client": "prisma generate --schema=./prisma/schema.postgresql.prisma"
  },
  "prisma": {
    "seed": "babel-node prisma/seed.js"
  },
  "jest": {
    "testEnvironment": "node",
    "globalSetup": "./tests/jest/globalSetup.js",
    "globalTeardown": "./tests/jest/globalTeardown.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/suhy-jang/tweetx.git"
  },
  "author": "Suhyeon Jang",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/suhy-jang/tweetx/issues"
  },
  "homepage": "https://github.com/suhy-jang/tweetx#readme",
  "dependencies": {
    "@apollo/client": "^3.8.3",
    "@apollo/server": "^4.9.3",
    "@aws-sdk/client-s3": "^3.410.0",
    "@babel/polyfill": "^7.10.4",
    "@prisma/client": "^5.3.0",
    "@sendgrid/mail": "^7.2.3",
    "bcryptjs": "^2.4.3",
    "core-js": "^3.6.5",
    "cross-fetch": "^4.0.0",
    "express": "^4.18.2",
    "express-graphql": "^0.12.0",
    "express-rate-limit": "^7.0.0",
    "graphql": "^0.13.2 || 16.x || 15.x",
    "graphql-cli": "^4.1.0",
    "graphql-yoga": "^4.0.4",
    "helmet": "^7.0.0",
    "jsonwebtoken": "^9.0.2",
    "nexus": "^1.3.0",
    "prisma": "^5.3.0",
    "react-scripts": "^5.0.1",
    "subscriptions-transport-ws": "^0.11.0",
    "xss": "^1.0.8"
  },
  "devDependencies": {
    "@babel/cli": "^7.10.5",
    "@babel/core": "^7.11.1",
    "@babel/node": "^7.10.5",
    "@babel/preset-env": "^7.11.0",
    "concurrently": "^8.2.1",
    "env-cmd": "^10.1.0",
    "jest": "^29.7.0",
    "node-notifier": "^10.0.1",
    "nodemon": "^3.0.1",
    "regenerator-runtime": "^0.14.0"
  }
}
